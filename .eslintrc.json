{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "eol-last": "off",
        "no-underscore-dangle": "off",
        "linebreak-style": "off",
        "class-methods-use-this": "off",
        "comma-dangle": "off",
        "no-param-reassign": "off",
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "no-useless-return": "off"
    }
}
